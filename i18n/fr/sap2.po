#: ../../sap2.c:735
#, c-format
msgid ""
"Usage:\n"
"\n"
"%s without parameters enters the interactive mode\n"
"\n"
"    %s -hv\n"
"    %s -tup archiv.sap [drive] [density]\n"
"    %s -c archive.sap [tracks] [density]\n"
"    %s -df drive [density] [interleave]\n"
"\n"
"  -c, --create        create an empty SAP archiv\n"
"  -d, --dir           show the directory of a Thomson floppy disk\n"
"  -f, --format        format a disk with Thomson format\n"
"  -h, --help          Show this help text\n"
"  -p, --pack          Archive a Thomson floppy disk to a SAP archiv\n"
"  -t, --list          View the contents of an SAP archiv\n"
"  -u, --unpack        Unpack a SAP archive to a Thomson disk\n"
"  -v, --version       Print the version of the program plus a copyright and "
"a list of  authors\n"
msgstr ""
"Usage:\n"
"\n"
"%s Programme sans paramètres démarre le mode interactif\n"
"\n"
"    %s -hv\n"
"    %s -tup archive.sap [lecteur] [densité]\n"
"    %s -c archive.sap [nb pistes] [densité]\n"
"    %s -df lecteur [densité] [entrelacement]\n"
"\n"
"  -c, --create        crée une archive SAP vide\n"
"  -d, --dir           affiche le contenu d'une disquette Thomson\n"
"  -f, --format        formate une disquette au format Thomson\n"
"  -h, --help          affiche cette aide\n"
"  -p, --pack          archive une disquette Thomson vers une archive SAP\n"
"  -t, --list          affiche la liste des fichiers de l'archive SAP\n"
"  -u, --unpack        désarchive une archive SAP vers une disquette Thomson\n"
"  -v, --version       affiche la version du programme\n"

#: ../../sap2.c:687
#, c-format
msgid ""
"Usage: %s  -h --help | -v --version | -t --list | -p --pack | -u --unpack\n"
